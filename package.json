  "dependencies": {
    "bcrypt": "^3.0.2",
    "env-cmd": "^8.0.2",
    "graphql": "^14.0.2",
    "graphql-yoga": "^1.3.2",
    "jsonwebtoken": "^8.4.0",
    "prisma-binding": "^2.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/graphql": "^14.0.5",
    "@types/jsonwebtoken": "^8.3.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "typescript": "^3.3.3"
  }
}