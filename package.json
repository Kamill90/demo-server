{
  "name": "demo-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "10.15.1",
    "npm": "5.6.0",
    "yarn": "1.7.0"
  },
  "scripts": {
    "start": "env-cmd ./config/prod.env node dist/index.js",
    "start:dev": "env-cmd ./config/dev.env nodemon dist/index.js",
